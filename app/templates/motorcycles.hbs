{{#if media.isMobile}}
{{#burger-menu as |burger|}}
  {{#burger.menu dismissOnItemClick="True" open=false itemTagName="li" as |menu|}}
    <a href="#" class="pull-right globalpadding"{{action burger.state.actions.close}}><i class="fa fa-lg fa-times" aria-hidden="true"></i></a>
    <h2>
    <img src="assets/images/icon.png" height="96px"/>
</h2>
<h2>Motoshare</h2>
    <ul>
      {{#menu.item}}
        {{link-to 'Find Motorcycles' 'motorcycles'}}
      {{/menu.item}}
       {{#menu.item}}
        {{link-to 'My Motorcycles' 'app.mymotorcycles'}}
      {{/menu.item}}
      {{#menu.item}}
        {{link-to 'Account Info' 'app.profile'}}
      {{/menu.item}}
{{!--       {{#menu.item}}
        {{link-to 'Log In' 'login'}}
      {{/menu.item}}
      {{#menu.item}}
        {{link-to 'Register' 'register'}}
      {{/menu.item}} --}}
    </ul>
  {{/burger.menu}}

  {{#burger.outlet}}
  <div class="mobilebar">
  <a href="#" class="icon"{{action burger.state.actions.toggle}}>
      <img src="/assets/images/icon.png" height="50px">
    </a>
    <div class="mobilebaritem">
         {{input type="text" class="form-control" value=zip placeholder="Enter Zip Code" focus-in='focused'}}
        </div>
        <div class="mobilebaritem">
        <button type="submit" class="btn btn-default"{{action 'searchZip'}}>Search</button>
      </div>
          {{#if errors}}
          <div class="mobilebaritem">
          <h5 class="text-danger">{{errors-text errors}}</h5>
        </div>
          {{/if}}
    </div>


{{#if map}}
{{#cycle-map latitude=lat longitude=lng}}
{{#each model as |motorcycle| }}
{{/each}}
{{/cycle-map}}
{{else}}
<h2>List View</h2>
{{/if}}
{{outlet}}
  {{/burger.outlet}}
{{/burger-menu}}

